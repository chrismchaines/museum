<h1><%= @gallery.name %></h1>
<h3><%= @gallery.city %></h3>

<ul>
  <% @gallery.paintings.each do |painting| %>
    <li>
      <%= link_to painting.title, painting_path(painting) %>
    </li>
  <% end %>
</ul>

<%= link_to "Full Paintings List", paintings_path %>
<br>
<%= link_to "Edit Gallery", edit_gallery_path(@gallery) %>